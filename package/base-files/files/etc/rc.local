iwconfig wlan0 txpower 23
iwconfig wlan1 txpower 23
uci set firewall.@rule["$(uci show firewall | grep 'Allow-ICMPv6-Forward' | cut -d'[' -f2 | cut -d']' -f1)"].enabled='0'
uci commit firewall
exit 0
